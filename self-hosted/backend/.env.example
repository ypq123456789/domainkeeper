# DomainKeeper 后端配置

# 服务器配置
PORT=3001
NODE_ENV=production

# JWT 密钥 (请务必更改为复杂的密钥)
JWT_SECRET=your_super_secret_jwt_key_change_this_in_production

# 管理员账户
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_admin_password

# 前台访问密码 (可选，留空表示不需要密码)
ACCESS_PASSWORD=

# Cloudflare API 配置
CF_API_TOKEN=your_cloudflare_api_token

# WHOIS 代理服务地址 (可选)
WHOIS_PROXY_URL=https://whois.0o11.com

# 数据库文件路径
DATABASE_PATH=./data/domains.db

# 数据同步间隔 (分钟)
SYNC_INTERVAL=60

# 允许的前端域名 (CORS)
FRONTEND_URL=http://localhost:3000

# 自定义标题
CUSTOM_TITLE=我的域名管理

# 日志级别
LOG_LEVEL=info
